            <td class="ng-cloak">&nbsp;{{ todo.broader_linear_range }}</td>
          </tr>
        </tbody>
      </table> -->
    </div>
    <div class="col-md-6">
      <div class="demo-container" ng-app="pwdt" ng-controller="chartController">
        <div id="chart" dx-chart="chartOptions"></div>
      </div>
    </div>
  </div>
</div>
